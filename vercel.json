{
  "version": 2,
  "builds": [
    {
      "src": "landing-page/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ads.txt",
      "dest": "/ads.txt"
    },
    {
      "src": "/(.*\\.html)$",
      "dest": "/landing-page/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/landing-page/index.html"
    }
  ]
}
